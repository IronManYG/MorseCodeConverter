[run]
source = morse_code
omit = 
    */test_*.py
    */tests/*
    */__pycache__/*
    */__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    def main

[html]
directory = coverage_html_report